{
  "name": "contributte/dev",
  "description": "Tools for development with Nette and Tracy",
  "keywords": [
    "nette",
    "development",
    "shortcuts",
    "debugger",
    "functions"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/dev",
  "authors": [
    {
      "name": "Milan Felix Sulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=7.2",
    "contributte/tracy": "^0.5.1",
    "contributte/utils": "^0.5.0"
  },
  "require-dev": {
    "ninjify/qa": "^0.12",
    "ninjify/nunjuck": "^0.4"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Dev\\": "src"
    },
    "files": [
      "src/shortcuts.php"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "0.4.x-dev"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
